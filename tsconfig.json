{
  "compilerOptions": {
    "target": "ES5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "contentlayer/generated": [
        "./.contentlayer/generated"
      ],
      "cache": [
        "./src/lib/cache"
      ],
      "configs/*": [
        "./src/configs/*"
      ],
      "logger": [
        "./src/lib/logger"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "fetchers/*": [
        "./src/fetchers/*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "layouts/*": [
        "./src/components/Layouts/*"
      ],
      "package": [
        "./package.json"
      ],
      "pages/*": [
        "./src/app/(pages)/*"
      ],
      "presets/*": [
        "./presets/*"
      ],
      "providers/*": [
        "./src/providers/*"
      ],
      "styling/*": [
        "./src/app/styles/*"
      ],
      "tailwind": [
        "./src/hooks/tailwindUtils"
      ],
      "utils/*": [
        "./src/utils/*"
      ],
      "ui/*": [
        "./src/components/UI/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".contentlayer/generated",
    "src/constants/secHeaders.ks",
    "src/lib/logger.js"
  ],
  "exclude": [
    "node_modules"
  ]
}